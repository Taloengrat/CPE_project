<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PlayingMode.Feedback"
    android:background="#E9A540">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutlogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline9">

        <ImageView
            android:id="@+id/mainStar"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_centerHorizontal="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_language_black_24dp"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/star1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="300"
            app:layout_constraintCircleRadius="95dp"
            app:srcCompat="@drawable/hindstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/star2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="330"
            app:layout_constraintCircleRadius="100dp"
            app:srcCompat="@drawable/hindstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/star3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="360"
            app:layout_constraintCircleRadius="105dp"
            app:srcCompat="@drawable/hindstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/star4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="30"
            app:layout_constraintCircleRadius="100dp"
            app:srcCompat="@drawable/hindstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/star5"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="60"
            app:layout_constraintCircleRadius="95dp"
            app:srcCompat="@drawable/hindstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />


        <ImageView
            android:id="@+id/fullstar1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="300"
            app:layout_constraintCircleRadius="95dp"
            android:visibility="gone"
            app:srcCompat="@drawable/fullstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/fullstar2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="330"
            app:layout_constraintCircleRadius="100dp"
            app:srcCompat="@drawable/fullstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/fullstar3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="360"
            app:layout_constraintCircleRadius="105dp"
            app:srcCompat="@drawable/fullstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/fullstar4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="30"
            app:layout_constraintCircleRadius="100dp"
            app:srcCompat="@drawable/fullstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/fullstar5"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="60"
            app:layout_constraintCircleRadius="95dp"
            app:srcCompat="@drawable/fullstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/halfstar1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="300"
            app:layout_constraintCircleRadius="95dp"
            android:visibility="gone"
            app:srcCompat="@drawable/halfstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/halfstar2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="330"
            app:layout_constraintCircleRadius="100dp"
            app:srcCompat="@drawable/halfstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/halfstar3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="360"
            app:layout_constraintCircleRadius="105dp"
            app:srcCompat="@drawable/halfstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/halfstar4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="30"
            app:layout_constraintCircleRadius="100dp"
            app:srcCompat="@drawable/halfstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageView
            android:id="@+id/halfstar5"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            app:layout_constraintCircle="@id/mainStar"
            app:layout_constraintCircleAngle="60"
            app:layout_constraintCircleRadius="95dp"
            app:srcCompat="@drawable/halfstar"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <TextView
            android:id="@+id/missionName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/bold"
            android:text="Mission Name"

            android:textSize="35dp"
            app:layout_constraintEnd_toEndOf="@+id/mainStar"
            app:layout_constraintStart_toStartOf="@+id/mainStar"
            app:layout_constraintTop_toBottomOf="@+id/mainStar" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imUser"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_below="@+id/layoutlogo"
        android:layout_alignStart="@+id/layoutlogo"
        android:layout_alignEnd="@+id/layoutlogo"
        android:layout_gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/layoutlogo"
        app:srcCompat="@drawable/user1"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/userName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imUser"
        android:layout_alignStart="@+id/imUser"
        android:layout_alignEnd="@+id/imUser"
        android:fontFamily="@font/thin"
        android:text="Profile Name"
        android:textSize="35dp"
        app:layout_constraintEnd_toEndOf="@+id/imUser"
        app:layout_constraintStart_toStartOf="@+id/imUser"
        app:layout_constraintTop_toBottomOf="@+id/imUser" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <ImageView
        android:id="@+id/missionHome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="@+id/userName"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintStart_toStartOf="@+id/userName"
        app:layout_constraintTop_toBottomOf="@+id/userName"
        app:layout_constraintWidth_percent="0.2"
        app:srcCompat="@drawable/home"
        android:onClick="Onclickbacktohome"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="back to mission"
        android:textSize="25dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/thin"
        app:layout_constraintEnd_toEndOf="@+id/missionHome"
        app:layout_constraintStart_toStartOf="@+id/missionHome"
        app:layout_constraintTop_toBottomOf="@+id/missionHome" />


</androidx.constraintlayout.widget.ConstraintLayout>